{
  "nodejs-testing.include": ["./tests"],
  "nodejs-testing.exclude": ["**/node_modules/**"],
  "nodejs-testing.concurrency": 1,
  "nodejs-testing.runtimeArgs": ["--test", "--env-file=./test.env"],
  "nodejs-testing.env": {
    "NODE_ENV": "test",
    "TEST_RUNNER": "vscode",
    "SEEDING": "true"
  }
}
